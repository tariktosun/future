:@template"É!$bg-main-color: #D8D8D8;
$bg-secondary-color: #404040;
$card-height: 60px;
$card-width: 250px;
$sidebar-width: 275px;

* {
	margin: 0px;
    padding: 0px;
    font-family:Oxygen-Light;
}

/*
 __  _     _      _                
/ _\(_) __| | ___| |__   __ _ _ __ 
\ \ | |/ _` |/ _ \ '_ \ / _` | '__|
_\ \| | (_| |  __/ |_) | (_| | |   
\__/|_|\__,_|\___|_.__/ \__,_|_|   
                                   
*/

@mixin abs_sidebar_features($width, $color, $left){
	background-color: $color;
	height: 100%;
	width: $width;
	top: 0;
	left: $left;
}

#sidestream{
	z-index: 1;
	position: absolute;
	top: 0;
	left: 0;
	height: 100%;
}

#sidebar{
	@include abs_sidebar_features($sidebar-width, $bg-main-color, 0);
	#username{
		font-size: 20px;
	}
	.profile-pic{
		width: 100%;
		position: relative;
		text-align: center;
		img{
			cursor: pointer;
			border: 2px $bg-secondary-color;
			margin-top: 15px;
		}
		span{
		    display: block;
		}
		#stars{
			img{
				margin-top: 5px;
				width: 15px;
			}
		}
	}
}

.sidebar-break{
	background: url("/static/images/break.png");
	width: 100%;
	height: 2px;
	margin: 5px 0;
}

.sidebar-game-list-title{
	padding: 5px;
}

.sidebar-game-list{
	width: 100%;
}

#sidebar-options {
	position: absolute;
	bottom: 0;
	table {
	height: 45px;
	width: 100%;
	table-layout:fixed;
		td {
			cursor: pointer;
			text-align: center;
			width: 33%;
			img {
				height: 50px;
			}
		}
	}
}



/*
   _                          
  /_\  _ __ _ __ _____      __
 //_\\| '__| '__/ _ \ \ /\ / /
/  _  \ |  | | | (_) \ V  V / 
\_/ \_/_|  |_|  \___/ \_/\_/  
*/                              

#arrow{
	@include abs_sidebar_features(35px, #979797, 275px);
	display: block;
	position: absolute;
	img{
		position: absolute;
		width: 35px;
		top: 45%;
		cursor: pointer;
	}
}

#arrow-hover{
	@include abs_sidebar_features(35px, none, 0);
	position: absolute;
}

/*
   ___              _ 
  / __\__ _ _ __ __| |
 / /  / _` | '__/ _` |
/ /__| (_| | | | (_| |
\____/\__,_|_|  \__,_|

*/

.card{
	cursor: pointer;
	width: $card-width;
	height: $card-height;
	background-color: $bg-secondary-color;
	margin: 10px auto;
	display: block;
	.icon{
		float: left;
		height: inherit;
		width: $card-height;
		margin: 0;
		.skill{
			height: 25px;
			width: 25px;
			position: relative;
			top: $card-height - 25;
			float: right;
		}
	}
	.card-text{
		color: #FFF;
		.card-date{ 
			padding-left: 5px;
			float: left;
			display: block;
			}
		.card-location{ 
			padding-left: 5px;
			float: left;
		}
		.card-capacity{ 
			position: relative;
			float: right;
			padding: 5px;
		}
	}
	.cardTooltip{
		display: none;
		z-index: 1;
		background-color: $bg-main-color;
		-webkit-box-shadow: 0 6px 6px 0 black;
	   	-moz-box-shadow: 0 6px 6px 0 black;
	    box-shadow: 0 6px 6px 0 black;
		left:275px;
		width: 300px;
		height: 300px;
		position: absolute;
		.game-title{
			font-style: italic;
			font-weight: 200;
			font-size: 20px;

		}
		.textual-info{
			padding-top: 5px;
			width: 100%;
			clear: both;
			float: left;
		}
		.leave-button{
			clear: both;
			float: left;
		}
		.game-page-button{
			clear: both;
			float: left;
		}
	}
}

/*
   _       _     _     ___                     
  /_\   __| | __| |   / _ \__ _ _ __ ___   ___ 
 //_\\ / _` |/ _` |  / /_\/ _` | '_ ` _ \ / _ \
/  _  \ (_| | (_| | / /_\\ (_| | | | | | |  __/
\_/ \_/\__,_|\__,_| \____/\__,_|_| |_| |_|\___|
                                               
*/

.add-game-cont{
	background-color: rgba(0,0,0,0.5);
	z-index: 1;
	display: none;
	height: 100%;
	top: 0;
	right: 0;
	position: absolute;
	width: calc(100% - 275px - 35px);
	width: -moz-calc(100% - 275px - 35px);
	width: -webkit-calc(100% - 275px - 35px);
	text-align: center;
}

#add-game-screen{
	padding: 25px;
	opacity: 1.0;
	margin-top: 10%;
	font:italic bold 15px Candara, sans-serif;
	text-align: left;
	display: none;
	margin: 10% auto;
	background-color: white;
	width: calc(75% - 275px);
	width: -moz-calc(75% - 275px);
	width: -webkit-calc(75% - 275px);
	height: 420px;
}

/*
 __                     _         ___ _      _     _ 
/ _\ ___  __ _ _ __ ___| |__     / __(_) ___| | __| |
\ \ / _ \/ _` | '__/ __| '_ \   / _\ | |/ _ \ |/ _` |
_\ \  __/ (_| | | | (__| | | | / /   | |  __/ | (_| |
\__/\___|\__,_|_|  \___|_| |_| \/    |_|\___|_|\__,_|
*/

.search-field{
	float: right;
	top: 0;
	right: 0;
	height: 100%;
	width: calc(100% - 275px - 35px);
}

/*
   ___            __ _ _      
  / _ \_ __ ___  / _(_) | ___ 
 / /_)/ '__/ _ \| |_| | |/ _ \
/ ___/| | | (_) |  _| | |  __/
\/    |_|  \___/|_| |_|_|\___|
                              
*/

#profile{
	display: none;
	img{
		padding: 35px;
	}
	.image-container{
		width: 250px;
		height: 100%;
		border-right: 1px #222 inset;
		display: inline-block;
	}
	.search-query-field{
		height: 100%;
		top: 0;
		float: right;
		width: calc(100% - 275px);
		width: -moz-calc(100% - 275px);
		width: -webkit-calc(100% - 275px);
		textarea {
		    resize: none;
		    width: 90%;
		    height: 20px;
		    font-size:16px;
		    color: #ccc;
		    padding:10px;
		    margin: 0 auto;
		    display: inline;
		    overflow: auto;
		}
		textarea.searchBox{
		    height:15px;
		    padding-left:30px;
		    border:solid thin #0084B4;
		    width: 80%;
		    overflow:hidden;
		    font-size:14px;
		    -moz-border-radius:2px;
		    -webkit-border-radius:2px;
		    margin-top:20px;
		}
		.search{
		    background: url('/static/images/icn_search.png') 0 2px no-repeat;
		    position:absolute;
		    height:16px;
		    width:20px;
		    margin-top:30px;
		    margin-left:10px;
		}
		.game-history{
			overflow-y: scroll;
			width: inherit;
			position: fixed;
			.past-game{
				height: 100px;
				float: left;
				width: 60%;
				background-color: $bg-main-color;
				margin: 10px;		
			}
		}
	}
}


#game{
	background-color: yellow;	
	display: none;
}

/* __       _     _           
  / /  ___ | |__ | |__  _   _ 
 / /  / _ \| '_ \| '_ \| | | |
/ /__| (_) | |_) | |_) | |_| |
\____/\___/|_.__/|_.__/ \__, |
                        |___/ 
*/
#lobby{
}

.filters{
	margin-top: 8px;
	padding-top: 20px;
	margin: 0 auto;
	width: 80%;
	height: 100px;
	text-align: center;
	background-position: center center;
	div{
		float: left;
	}
	.styled-select {
	   width: 240px;
	   float: right;
	   height: 34px;
	   overflow: hidden;
	   background: url('/static/images/new-arrow.jpg') no-repeat right #ddd;
	   border: 1px solid #ccc;
	   select {
		   background: transparent;
		   width: 268px;
		   padding: 5px;
		   font-size: 16px;
		   line-height: 1;
		   border: 0;
		   border-radius: 0;
		   height: 34px;
		   -webkit-appearance: none;
		}
	}
}
.games-grid{
	margin-top: 15px;
	width: 100%;
	height: calc(100% - 100px);
	height: -moz-calc(100% - 100px);
	height: -webkit-calc(100% - 100px);
	overflow-y: scroll;
	position: fixed;
	.box{
		float: left;
		width: 250px;
		height: 250px;
		margin: 10px;
		background-color: $bg-main-color ;
	}
}

#friends-games{
	display: none;
}

@font-face
{
font-family: Oxygen;
src: url(/static/font/Oxygen-Light.ttf);
}

.buttons{
	padding: 8px;
}

.button {
	-moz-box-shadow:inset 0px 1px 0px 0px #ffffff;
	-webkit-box-shadow:inset 0px 1px 0px 0px #ffffff;
	box-shadow:inset 0px 1px 0px 0px #ffffff;
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #f9f9f9), color-stop(1, #e9e9e9) );
	background:-moz-linear-gradient( center top, #f9f9f9 5%, #e9e9e9 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#f9f9f9', endColorstr='#e9e9e9');
	background-color:#f9f9f9;
	-webkit-border-top-left-radius:14px;
	-moz-border-radius-topleft:14px;
	border-top-left-radius:14px;
	-webkit-border-top-right-radius:14px;
	-moz-border-radius-topright:14px;
	border-top-right-radius:14px;
	-webkit-border-bottom-right-radius:14px;
	-moz-border-radius-bottomright:14px;
	border-bottom-right-radius:14px;
	-webkit-border-bottom-left-radius:14px;
	-moz-border-radius-bottomleft:14px;
	border-bottom-left-radius:14px;
	text-indent:0;
	border:1px solid #dcdcdc;
	display:inline-block;
	color:#666666;
	font-family:Arial;
	font-size:13px;
	font-weight:normal;
	font-style:normal;
	height:36px;
	line-height:36px;
	width:86px;
	text-decoration:none;
	text-align:center;
	text-shadow:1px 1px 0px #ffffff;
}
.button:hover {
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #e9e9e9), color-stop(1, #f9f9f9) );
	background:-moz-linear-gradient( center top, #e9e9e9 5%, #f9f9f9 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#e9e9e9', endColorstr='#f9f9f9');
	background-color:#e9e9e9;
}.button:active {
	position:relative;
	top:1px;
